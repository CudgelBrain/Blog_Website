<div class="author">
    <h1 class="author_heading">
        search results
    </h1>
</div>

<section class="articles">
    
        <!-- <ul class="article_ul">
        <li>

            <a> <%= searchTerm %> </a>
        </li>
    </ul>
     -->
    <% data.forEach(post => { %>
        <ul class="article_ul">
         
            <li>
                <a href="/post/ <%= post._id %>">
                    <span><%= post.title %></span>
                    <span class="article_list_date"><%= post.createdAt.toDateString() %></span>
                </a>
            </li>
            <% }) %>
    </ul>
</section>